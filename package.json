{
	"name": "htl-language-support",
	"displayName": "htl-language-support",
	"publisher": "parkerpierpont",
	"repository": "https://github.com/parkerpierpont/HTL-Language-Support",
	"description": "",
	"version": "0.2.1",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "htl",
				"aliases": [
					"HTL",
					"htl",
					"HTML (HTL / Sightly / AEM)"
				],
				"extensions": [
					".html"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "htl",
				"scopeName": "text.html.htl.aem",
				"path": "./syntaxes/htl.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "htl",
				"path": "./snippets/htl-snippets.json"
			}
		]
	}
}
